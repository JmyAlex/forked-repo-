!-----------------------------------------------------------------------
! file:     ~/.Xresources
! author:   enko
! modified: nov 2009
! vim:enc=utf-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
!
! ----------------------------------------------------------------------
!
!            X F T    S E T T I N G S
!
Xft.dpi:                        96.0
Xft.hinting:                    true
Xft.hintstyle:                  hintslight
Xft.antialias:                  true
Xft.rgba:                       none
! ----------------------------------------------------------------------
!
!            X C U R S O R
!
Xcursor*theme:                  Vanilla-DMZ-AA
Xcursor.size:                   22 
! ----------------------------------------------------------------------
!
!            X T E R M          #прописывать здесь,в Xdefaults не срабатывает..
!
xterm*termName:             	xterm-256color
xterm*internalBorder:       	0
xterm*loginShell:           	true
xterm*scrollBar:            	false
xterm*cursorBlink:          	false
xterm*geometry:             	80x23
xterm*saveLines:            	65535
xterm*dynamicColors:        	on
xterm*highlightSelection:   	true
xterm*charClass:            	33:48,35-38:48,39:43,42-47:48,58-59:48,61:48,63-64:48,126:48
xterm*utf8:                 	2
xterm*eightBitInput:        	true
xterm*faceName:             	fixed:pixelsize=15
xterm*cursorColor:          	#dcdccc
xterm*shading:              	30
xterm*transparent:          	true
! ----------------------------------------------------------------------
! 
!            C O L O R    S E T T I N G S
!
